  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.72",
    "@types/chai": "4.2.15",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.27",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "all-contributors-cli": "6.19.0",
    "chai": "4.3.0",
    "eslint": "7.20.0",
    "eslint-plugin-mocha": "8.0.0",
    "mocha": "8.3.0",
    "nyc": "15.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  },
  "dependencies": {
    "aws-lambda": "1.0.6"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ]
  }
